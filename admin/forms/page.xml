<?xml version="1.0" encoding="utf-8"?><form>	<config>         		<inlinehelp button="show"/>     	</config>	<fieldset name="gene_tab" label="CG_GAL_GENE_TAB" addfieldprefix="ConseilGouz\Component\CGGallery\Administrator\Field">		<field name="version" type="version"  margintop="-3em" extension="com_cggallery"/>		<field name="id" type="hidden" default="0"			readonly="true" class="readonly"			label="JGLOBAL_FIELD_ID_LABEL" 	description ="JGLOBAL_FIELD_ID_DESC" />		<field name = "note1" type="note" label="CG_GAL_XML_DESCRIPTION" />		<field name="title" type="text"			label="CG_GAL_TITLE" 			description ="CG_GAL_TITLE_DESC" 			required="true"  />		<field	name="state" type="list" label="JSTATUS"			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"			size="1" default="1">			<option value="1">JPUBLISHED</option>			<option value="0">JUNPUBLISHED</option>			<option value="2">JARCHIVED</option>			<option value="-2">JTRASHED</option>		</field>		<field	name="language"	type="contentlanguage"			label="JFIELD_LANGUAGE_LABEL" description="JFIELD_LANGUAGE_DESC"	>			<option value="*">JALL</option>		</field>		<field	name="ug_compression" type="number"			default="15" min="10" max="100"	step="5"			label="CG_GAL_COMPRESS" description="CG_GAL_COMPRESSDESC" />		<field name="intro" type="editor" label="CG_GAL_INTRO" description="CG_GAL_INTRO_DESC" cols="400" rows="4" filter="safehtml" 			default=""/>			<field name="bottom" type="editor" label="CG_GAL_BOTTOM" description="CG_GAL_BOTTOM_DESC" cols="400" rows="4" filter="safehtml" 			default=""/>		</fieldset>	<fieldset name="layout_tab" label="CG_GAL_LAYOUT_TAB">		<field 	name = "noteaff" type="note" label="CG_GAL_GENENOTE" />		<field	name="ug_type"	type="list"	default="tiles"			label="CG_GAL_TYPE"	description="CG_GAL_TYPEDESC">			<option value="tiles">CG_GAL_TYPETILES</option>			<option value="grid">CG_GAL_TYPEGRID</option>			<option value="carousel">CG_GAL_TYPECAR</option>			<option value="slider">CG_GAL_TYPESLIDER</option>		</field>		<field 	name = "noteslider" type="note" label="CG_GAL_TYPESLIDERNOTE"				showon="ug_type:slider"	/>		<field 	name="ug_tiles_type" type="list"			showon="ug_type:tiles"			label="CG_GAL_TYPETILES_TYPE"	description="CG_GAL_TYPETILES_TYPEDESC"	>				<option value="column">CG_GAL_TYPETILES_COL</option>				<option value="justified">CG_GAL_TYPETILES_JUST</option>				<option value="nested">CG_GAL_TYPETILES_NEST</option>				<option value="tilesgrid">CG_GAL_TYPETILES_GRID</option>		</field>		<field name="ug_tile_width" type="text" default="200"			class="validate-numeric"			showon="ug_type:tiles[AND]ug_tiles_type:nested[OR]ug_type:carousel,slider"			label="CG_GAL_TILE_WIDTH" description="CG_GAL_TILE_WIDTHDESC"	/>		<field 	name="ug_min_columns"	type="text"	default="2"			showon="ug_type:tiles[AND]ug_tiles_type:column"			label="CG_GAL_MIN_COL"	class="validate-numeric" description="CG_GAL_MIN_COLDESC" />		<field 	name="ug_tile_height" type="text" default="150" class="validate-numeric"			showon="ug_type:tiles[AND]ug_tiles_type:justified,tilesgrid[OR]ug_type:carousel,slider"			label="CG_GAL_IMGHEIGHT" description="CG_GAL_IMGHEIGHTDESC"	/>		<field name="ug_grid_num_rows"	type="text"	default="1"	class="validate-numeric"			showon="ug_type:tiles[AND]ug_tiles_type:tilesgrid"			label="CG_GAL_NUMROWS"	description="CG_GAL_NUMROWSDESC" />		<field name="ug_space_between_rows"	type="text"	default="15" class="validate-numeric"			showon="ug_type:tiles[AND]ug_tiles_type:tilesgrid,grid"			label="CG_GAL_SPACEROW"	description="CG_GAL_SPACEROWDESC" />		<field 	name="ug_space_between_cols" type="text" default="15" class="validate-numeric"			showon="ug_type:tiles,grid,carousel"			label="CG_GAL_SPACECOL" description="CG_GAL_SPACECOLDESC" />		<field 	name="ug_carousel_autoplay_timeout"	type="text"	default="5000"	class="validate-numeric"			showon="ug_type:carousel,slider"			label="CG_GAL_DELAY" description="CG_GAL_DELAYDESC"	/>		<field 	name="ug_carousel_scroll_duration"	type="text"	default="500"	class="validate-numeric"			showon="ug_type:carousel,slider"			label="CG_GAL_TRANSITION" description="CG_GAL_TRANSITIONDESC"/>		<field name="ug_texte"	type="list" 			label="CG_GAL_TEXT"	description="CG_GAL_TEXTDESC">			<option value="true">CG_GAL_TEXTTRUE</option>			<option value="fixe">CG_GAL_TEXTFIXE</option>			<option value="false">CG_GAL_TEXTHIDE</option>		</field>		<field name="ug_text_lgth" type="text"	default="100"			showon="ug_texte:true"			label="CG_GAL_TEXTLGTH" description="CG_GAL_TEXTLGTHDESC"/>		<field 	name="ug_link"	type="list"	default="false"			label="CG_GAL_LINK"	description="CG_GAL_LINKDESC">			<option value="true">JYES</option>			<option value="false">JNO</option>		</field>		<field 			name="ug_zoom" showon="ug_type:grid,slider"			type="list"		default="true"			label="CG_GAL_ZOOM" description="CG_GAL_ZOOMDESC">			<option value="true">JYES</option>			<option value="false">JNO</option>		</field>		<field name="ug_lightbox" type="list" default="true"			label="CG_GAL_LIGHTBOX"	description="CG_GAL_LIGHTBOXDESC">			<option value="true">JYES</option>			<option value="false">JNO</option>		</field>		<field name="ug_grid_thumbs_pos" type="list" default="right"			showon="ug_type:grid"			label="CG_GAL_THUMBS_POS"	description="CG_GAL_THUMBS_POSDESC">			<option value="left">CG_GAL_POS_LEFT</option>			<option value="right">CG_GAL_POS_RIGHT</option>			<option value="top">CG_GAL_POS_TOP</option>			<option value="bottom">CG_GAL_POS_BOTTOM</option>		</field>		<field name="ug_grid_show_icons" type="list" default="true"			showon="ug_type:grid"			label="CG_GAL_GRID_ICONS"	description="CG_GAL_GRID_ICONSDESC">			<option value="true">JYES</option>			<option value="false">JNO</option>		</field>	</fieldset>	<fieldset name="images_tab" label="CG_GAL_IMAGES_TAB">		<field 	name = "notedir" type="note" label="CG_GAL_DIRECTORYNOTE" />		<field 	name="ug_dir_or_image"	type="list"			label="CG_GAL_CHOOSE" description="CG_GAL_CHOOSEDESC"	>			<option value="dir">CG_GAL_CHOOSEDIR</option>			<option value="file">CG_GAL_CHOOSEFILE</option>		</field>		<field 	name = "notedirinit" type="note" label="CG_GAL_CHOOSEDIRNOTE"			showon="ug_dir_or_image:dir"/>		<field name="ug_autothumb" type="hidden" value="1" />		<field 	name="ug_big_dir"  type="folderlist"			default="images" recursive="true"			exclude="^administrator|^api|^cli|^components|^includes|^language|^layouts|^libraries|^modules|^plugins"			label="CG_GAL_DIR" description="CG_GAL_DIRDESC"			showon="ug_dir_or_image:dir"/>		<field name="ug_full_dir" type="list"			label="CG_GAL_DIRFULL"	description="CG_GAL_DIRFULLDESC"			showon="ug_dir_or_image:dir">			<option value="true">JYES</option>			<option value="false">JNO</option>		</field>		<field name="ug_file_nb" type="text"			showon="ug_dir_or_image:dir[AND]ug_full_dir:false"			default="5"	class="validate-numeric"			label="CG_GAL_DIRNBR" description="CG_GAL_DIRNBRDESC"/>		<field name = "noteimage" type="note" label="CG_GAL_IMGNOTE"			showon="ug_dir_or_image:file"/>		<field	showon="ug_dir_or_image:file"			name="ug_articles" type="radio"	default="articles"	class="btn-group"				label="CG_GAL_ENTREE" description="CG_GAL_ENTREEDESC" >				<option value="articles">CG_GAL_ENTREEARTICLES</option> 				<option value="k2">K2</option> 		</field>				<field name="slideslist" label="" type="subform" multiple ="true" 			showon="ug_dir_or_image:file[AND]ug_articles:articles"			formsource="administrator/components/com_cggallery/forms/slidelist.xml" 		/>			</fieldset></form>